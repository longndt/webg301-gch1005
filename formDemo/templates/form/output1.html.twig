<table border="1">
   <tr>
      <th>Salutation</th>
      <td>{{salutation}}</td>
   </tr>
   <tr>
      <th>Name</th>
      <td>{{name}}</td>
   </tr>
   <tr>
      <th>Gender</th>
      <td>{{gender}}</td>
   </tr>
   <tr>
      <th>Email</th>
      <td>{{email}}</td>
   </tr>
   <tr>
      <th>Birthday</th>
      <td>{{dob | date('d/m/Y')}}</td>
   </tr>
   <tr>
      <th>Address</th>
      <td>{{address}}</td>
   </tr>
</table>